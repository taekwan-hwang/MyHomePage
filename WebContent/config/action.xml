<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE persons SYSTEM "action.dtd">
<actions>
	<action path="/main.do" className="action.MainAction">
	<result type="dispatcher">/main.jsp</result>
	</action>
	<action path="/login/loginform.do" className="action.LoginFormAction">
		<result type="dispatcher">/login/loginform.jsp</result>
	</action>
	<action path="/login/login.do" className="action.LoginAction">
		<result type="dispatcher"/>
	</action>
	<action path="/login/signupform.do" className="action.SignUpFormAction">
		<result type="dispatcher">/login/signupform.jsp</result>
	</action>
	<action path="/login/signup.do" className="action.SignUpAction">
		<result type="redirect">/main.jsp</result>
	</action>
</actions>